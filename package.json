{
  "name": "digit-ict-conf-2016",
  "description": "DIGIT-ICT Conference 2016 website",
  "version": "1.12.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ec-europa/digit-ict-conf-2016.git"
  },
  "author": "",
  "license": "EUPL-1.1",
  "bugs": {
    "url": "https://github.com/ec-europa/digit-ict-conf-2016/issues"
  },
  "homepage": "https://github.com/ec-europa/digit-ict-conf-2016#readme",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.prod.babel.js --color -p && node ./tools/scripts/static.js",
    "build:clean": "rimraf ./build/*",
    "start": "webpack-dashboard -- cross-env NODE_ENV=development node tools/scripts/server.js",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:md && npm run lint:scss",
    "lint:js": "eslint . --ignore-pattern tools/scripts",
    "lint:json": "eslint . --ext .json",
    "lint:md": "remark .",
    "lint:scss": "stylelint ./app/**/*.scss",
    "lint-staged": "lint-staged",
    "release": "git checkout master && git pull origin master && standard-version && git push --follow-tags origin master && conventional-github-releaser -p angular",
    "serve": "cross-env NODE_ENV=production node tools/scripts/prodServer.js"
  },
  "lint-staged": {
    "*.js": [
      "lint:js"
    ],
    "*.scss": [
      "lint:scss"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "classnames": "2.2.5",
    "headroom.js": "0.9.3",
    "history": "2.1.2",
    "mappy-breakpoints": "0.2.3",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-dom": "15.4.1",
    "react-flip-move": "2.7.1",
    "react-helmet": "3.2.2",
    "react-redux": "4.4.6",
    "react-router": "2.8.1",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "redux-persist": "3.5.0",
    "redux-thunk": "2.1.0",
    "sanitize.css": "4.1.0",
    "scriptjs": "2.5.8",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "typi": "2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
